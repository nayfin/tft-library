{
  "name": "tft-library-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build tft-library",
    "watch-lib": "ng build tft-library --watch",
    "work": "bash ./scripts/work.sh",
    "npm-pack": "cd dist/tft-library && npm pack",
    "package": "npm run build-lib && npm run npm-pack && npm run schematics:build",
    "release": "bash ./scripts/release.sh",
    "schematics:build": "tsc -p projects/tft-library/tsconfig.schematics.json",
    "compodoc:lib": "compodoc -p projects/tft-library/tsconfig.lib.json -s -o -w",
    "compodoc:app": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s --port 8081"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/fire": "^5.2.3",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@firebase/app": "^0.4.25",
    "@tft/interact": "0.2.1",
    "angular-instantsearch": "^2.2.2",
    "core-js": "^3.4.2",
    "firebase": "7.5.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.10",
    "rxjs": "^6.5.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.0",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "^8.3.19",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.12",
    "codelyzer": "^5.2.0",
    "copy-webpack-plugin": "^5.0.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.7.1",
    "protractor": "^5.4.1",
    "ts-node": "~8.5.2",
    "tsickle": "^0.37.1",
    "tslib": "^1.9.3",
    "tslint": "~5.20.1",
    "typescript": "~3.5.0",
    "webpack-node-externals": "^1.7.2"
  }
}
